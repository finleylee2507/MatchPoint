{
  "name": "vite-template-react",
  "version": "1.2.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit --only database,auth",
    "em:exec": "firebase emulators:exec --import=./saved-data 'npm start' --only database,auth",
    "em:execui": "firebase emulators:exec --ui --import=./saved-data 'npm start' --only database,auth",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "bootstrap": "^5.2.3",
    "firebase": "^9.15.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "react-toastify": "^9.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "c8": "^7.12.0",
    "cypress": "^12.5.0",
    "jsdom": "^20.0.3",
    "vite": "^4.0.0"
  }
}
